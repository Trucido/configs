! $Id: .Xdefaults,v 2.0 2013/12/11 04:22:28 xoddf2 Exp $

Xft.dpi: 96

! xterm ------------------------------------------------------------------------
XTerm.vt100.locale: UTF-8
XTerm.vt100.font: -misc-fixed-medium-r-normal--14-*-*-*-*-*-iso10646-1
XTerm.vt100.utf8Fonts.font: -misc-fixed-medium-r-normal--14-*-*-*-*-*-iso10646-1
XTerm.vt100.wideFont: -*-*-*-*-*-ja-*-*-*-*-*-*-*-*
XTerm.vt100.reverseVideo: true
XTerm.vt100.altIsNotMeta: true
XTerm.vt100.altSendsEscape: true
XTerm.vt100.scrollBar: false
XTerm.vt100.bellIsUrgent: true
XTerm.vt100.backarrowKey: false

! Emacs  -----------------------------------------------------------------------
emacs.font: -xos4-terminus-medium-r-*-*-14-*-*-*-*-*-*-*
emacs.background: black
emacs.foreground: grey
emacs.menuBar: off
emacs.verticalScrollBars: off
emacs.toolBar: off
emacs.internalBorder: 0
