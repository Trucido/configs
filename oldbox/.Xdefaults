! $Id: .Xdefaults,v 1.3.2 2013/11/14 06:21:54 xoddf2 Exp $

Xft.dpi: 96

! xterm ------------------------------------------------------------------------
XTerm.vt100.locale: UTF-8
XTerm.vt100.font: -misc-fixed-medium-r-normal--15-*-*-*-*-*-iso10646-1
XTerm.vt100.utf8Fonts.font: -misc-fixed-medium-r-normal--15-*-*-*-*-*-iso10646-1
XTerm.vt100.wideFont: -*-*-*-*-*-ja-*-*-*-*-*-*-*-*
XTerm.vt100.reverseVideo: true
XTerm.vt100.altIsNotMeta: true
XTerm.vt100.altSendsEscape: true
XTerm.vt100.scrollBar: true
XTerm.vt100.bellIsUrgent: true
XTerm.vt100.backarrowKey: false

! Emacs  -----------------------------------------------------------------------
emacs.font: -adobe-courier-medium-r-*-*-14-*-*-*-*-*-*-*
emacs.geometry: 80x50
emacs.background: black
emacs.foreground: grey
emacs.menuBar: off
emacs.verticalScrollBars: off
emacs.toolBar: off
emacs.internalBorder: 0

! xclock -----------------------------------------------------------------------
XClock.Clock.background: black
XClock.Clock.foreground: green
XClock.Clock.analog: false
XClock.Clock.update: 1
XClock.Clock.strftime: %a %b %d %H:%M:%S %Y
XClock.Clock.face: Fixed-13:foundry=misc:weight=regular:slant=roman
XClock.Clock.render: true

! xload ------------------------------------------------------------------------
XLoad*font: -misc-fixed-bold-r-*-*-13-*-*-*-*-*-*-*
XLoad*background: black
XLoad*foreground: white
XLoad*highlight: red

! xbiff ------------------------------------------------------------------------
XBiff.Mailbox.checkCommand: fetchmail -d0 -csk
XBiff.Mailbox.update: 300
XBiff.Mailbox.background: black
XBiff.Mailbox.foreground: green
