! .Xresources 2.2.1   Time-stamp: <2014-04-22 15:27:21 PDT xoddf2>

Xft.dpi: 96

! xterm ------------------------------------------------------------------------
XTerm*locale: UTF-8
XTerm*font: -misc-fixed-medium-r-normal--13-*-*-*-*-*-iso10646-1
XTerm*utf8Fonts.font: -misc-fixed-medium-r-normal--13-*-*-*-*-*-iso10646-1
XTerm*wideFont: -*-*-*-*-*-ja-*-*-*-*-*-*-*-*
XTerm*background: black
XTerm*foreground: white
XTerm*termName: xterm-256color
XTerm*altIsNotMeta: true
XTerm*altSendsEscape: true
XTerm*scrollBar: false
XTerm*bellIsUrgent: true
XTerm*backarrowKey: false
XTerm*ttyModes: erase ^?
XTerm*Translations: #override <Key>BackSpace: string (0x7f) \n<Key>Delete: string (0x1b) string("[3~")

! Emacs ------------------------------------------------------------------------
emacs*font: -xos4-terminus-medium-r-*-*-16-*-*-*-*-*-*-*
emacs*geometry: 80x55
emacs*background: black
emacs*foreground: grey
emacs*menuBar: off
emacs*verticalScrollBars: off
emacs*toolBar: off
emacs*internalBorder: 0

! xclock -----------------------------------------------------------------------
XClock.Clock.background: black
XClock.Clock.foreground: green
XClock.Clock.analog: false
XClock.Clock.update: 1
XClock.Clock.face: Fixed-8:foundry=misc:weight=normal:slant=roman
XClock.Clock.padding: 1
XClock.Clock.render: true
XClock.Clock.strftime: %a %b %d %H:%M:%S %Z %Y
